                <div class="c-modal__inner">
                  <section class="c-modal__wrap">
                    <div class="c-modal__openInner">
                      <div class="c-modal__openWrap">
                        <button class="c-modal__open" data-custom-open="modal-1" role="button">Modal Open</button>
                      </div>
                    </div>
                    <div id="modal-1" class="c-modal" aria-hidden="true">
                      <div class="c-modal__overlay" tabindex="-1" data-custom-close="modal-1">
